#include<stdio.h>
int main()
{
    int i,count_even=0,count_odd=0;
    for(i=1;i<=10;i++)
    {
        if(i%2==0)
        {
            count_even++;
        }
        else
        {
            count_odd++;
        }
    }
    printf(" Even no count=%d\n",count_even);
    printf(" Odd no count=%d",count_odd);
    return 0;
}
